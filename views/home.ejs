<%- include("partials/header") %>
      <section id="home">
        <div class="container-fluid">
          <div class="row">
            <div class="col-lg-3">

            </div>
            <div class="col-lg-6">
              <div class="Salutation">
                <div class="content">

                  </div>
                <div class="show-q">Post your questions and get clarified</div>
                <div class="wrap-ask-q">
                  <div class="profile-pic">
                    <% if(User.userphoto){ %>
                    <a href="/profile/<%- User._id %>"><img class="" src="<%- User.userphoto %>" alt="user-img"></a>
                    <% }else{ %>
                      <a href="/profile/<%- User._id %>"><i class="fas fa-user-circle"></i></a>
                    <% } %>
                    <div class="ask-q">
                      <input class="border" type="text" name="<%- User._id %>" placeholder="Ask Question" data-toggle="modal" data-target="#exampleModalCenter">
                    </div>
                  </div>
                </div>
              </div>
              <div class="home-content">

              </div>
            </div>
            <div class="col-lg-3">
              <div class="show-stats">

              </div>
            </div>
          </div>
        </div>
      </section>
<script src="/js/home.js" charset="utf-8"></script>
<%- include("partials/footer") %>
